{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
         "no-console": "off",
         "comma-dangle": "off",
         "quotes": "off",
         "react/prop-types": 0,
         "arrow-body-style": 0,
         "space-before-function-paren": 0
    }
}